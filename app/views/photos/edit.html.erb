<h1>Editing photo</h1>

<% form_for(@photo) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :image_filename %><br />
    <%= f.text_field :image_filename %>
  </p>
  <p>
    <%= f.label :image_width %><br />
    <%= f.text_field :image_width %>
  </p>
  <p>
    <%= f.label :image_height %><br />
    <%= f.text_field :image_height %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @photo %> |
<%= link_to 'Back', photos_path %>